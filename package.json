{
  "name": "@eatonfyi/mailstats",
  "version": "1.0.0",
  "description": "Ingest a huge pile of archived mailboxes for analysis.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "build": "pkgroll",
    "demo": "npm run clean && tsimp ./src/demo.ts",
    "clean": "shx rm -rf dist && shx rm -rf .tsimp",
    "prepare": "npm run clean && npm run build",
    "migrate": "drizzle-kit push:sqlite",
    "generate": "drizzle-kit generate:sqlite",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "author": "eaton",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "ava": "^6.1.2",
    "drizzle-kit": "^0.20.14",
    "pkgroll": "^2.0.2",
    "shx": "^0.3.4",
    "tsimp": "^2.0.11",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@eatonfyi/fs-jetpack": "github:eaton/fs-jetpack",
    "@eatonfyi/ids": "github:eaton/ids",
    "@eatonfyi/mbox-streamer": "github:eaton/mbox-streamer",
    "@eatonfyi/urls": "github:eaton/urls",
    "@thi.ng/mime": "^2.6.4",
    "@types/better-sqlite3": "^7.6.9",
    "@types/mailparser": "^3.4.4",
    "better-sqlite3": "^9.4.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.7",
    "mailparser": "^3.6.9"
  }
}
